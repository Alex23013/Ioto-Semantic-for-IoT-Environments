:hasEthicMode rdf:type owl:ObjectProperty ;
    rdfs:domain ssn:System ;
    rdfs:range :EthicMode ;
    rdfs:isDefinedBy ioto: .

:RecommendedSystem rdf:type owl:Class ;
    owl:intersectionOf (
        ssn:System
        [ rdf:type owl:Restriction ;
          owl:onProperty :hasEthicCategory ;
          owl:hasValue :Recommended
        ]
    ) ;
    rdfs:isDefinedBy ioto: .

[ rdf:type owl:Class ;
  owl:intersectionOf (
    :RecommendedSystem
    [ rdf:type owl:Restriction ;
      owl:onProperty :hasEthicMode ;
      owl:someValuesFrom ioto:Mild
    ]
  )
] rdfs:subClassOf :RecommendedSystem .

