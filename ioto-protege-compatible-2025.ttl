      rdfs:subClassOf owl:Thing ;
      :hasName xsd:string ;
      rdfs:isDefinedBy ioto: .

ioto:isProtectedIn rdf:type owl:ObjectProperty ;
    rdfs:domain ioto:LegalRight ;
    rdfs:range ioto:Country ;
    rdfs:isDefinedBy ioto: .

##### EXAMPLE of USE

:iotoChile rdf:type ioto:Country ;
    :hasName "Chile" ;
    rdfs:isDefinedBy ioto: .

:cancellation rdf:type ioto:LegalRight ;
    ioto:isProtectedIn :iotoChile ;
    rdfs:isDefinedBy ioto: .

:access rdf:type ioto:LegalRight ;
    ioto:isProtectedIn :iotoChile ;
    rdfs:isDefinedBy ioto: .

:rectification rdf:type ioto:LegalRight ;
    ioto:isProtectedIn :iotoChile ;
    rdfs:isDefinedBy ioto: .

:opposition rdf:type ioto:LegalRight ;
    ioto:isProtectedIn :iotoChile ;
    rdfs:isDefinedBy ioto: .

:portability rdf:type ioto:LegalRight ;
    ioto:isProtectedIn :iotoChile ;
    rdfs:isDefinedBy ioto: .

