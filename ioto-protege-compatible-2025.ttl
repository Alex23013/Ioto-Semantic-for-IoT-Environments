#################################################################
ioto:IoTDevice a rdfs:Class ;
    rdfs:label "IoT Device" ;
    rdfs:comment "Represents a device in an IoT system that contains sensors." .

# Define ioto:LegacyDevice as a subclass of IoTDevice
ioto:LegacyDevice a rdfs:Class ;
    rdfs:subClassOf ioto:IoTDevice ;
    rdfs:label "Legacy IoT Device" ;
    rdfs:comment "An IoT device that uses legacy data formats like XML or CSV." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ioto:hasSensor ;
        owl:someValuesFrom [
            a owl:Class ;
            owl:intersectionOf (
                sosa:Sensor
                [ a owl:Restriction ;
                  owl:onProperty ioto:usesDataFormat ;
                  owl:hasValue ioto:XML
                ]
                [ a owl:Restriction ;
                  owl:onProperty ioto:usesDataFormat ;
                  owl:hasValue ioto:CSV
                ]
            )
        ]
    ] .

# Define ioto:ModernDevice as a subclass of IoTDevice
ioto:ModernDevice a rdfs:Class ;
    rdfs:subClassOf ioto:IoTDevice ;
    rdfs:label "Modern IoT Device" ;
    rdfs:comment "An IoT device that uses modern data formats like JSON, RDF, or Turtle." ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty ioto:hasSensor ;
        owl:someValuesFrom [
            a owl:Class ;
            owl:intersectionOf (
                sosa:Sensor
                [ a owl:Restriction ;
                  owl:onProperty ioto:usesDataFormat ;
                  owl:hasValue ioto:JSON
                ]
                [ a owl:Restriction ;
                  owl:onProperty ioto:usesDataFormat ;
                  owl:hasValue ioto:RDF
                ]
                [ a owl:Restriction ;
                  owl:onProperty ioto:usesDataFormat ;
                  owl:hasValue ioto:Turtle
                ]
            )
        ]
    ] .
    
# Define the DataFormat class
ioto:DataFormat a rdfs:Class ;
    rdfs:label "Data Format" ;
    rdfs:comment "Represents the data format used for communication and interoperability in IoT systems." .

# Define the object property hasSensor (IoTDevice → Sensor) #Check if system already don't have Sensor or which class has relationship with Sensor
ioto:hasSensor a owl:ObjectProperty ;
    rdfs:domain ioto:IoTDevice ;
    rdfs:range sosa:Sensor ;
    rdfs:label "has Sensor" ;
    rdfs:comment "Links an IoT device to its sensors." .

# Define the object property usesDataFormat (Sensor → DataFormat)
ioto:usesDataFormat a owl:ObjectProperty ;
    rdfs:domain sosa:Sensor ;
    rdfs:range ioto:DataFormat ;
    rdfs:label "uses Data Format" ;
    rdfs:comment "Defines the data format used by a sensor for interoperability." .

# Define some common data formats
ioto:JSON a ioto:DataFormat ;
    rdfs:label "JSON" ;
    rdfs:comment "JavaScript Object Notation, a widely used data format for IoT communication." .

ioto:XML a ioto:DataFormat ;
    rdfs:label "XML" ;
    rdfs:comment "Extensible Markup Language, another standard data format for IoT." .

ioto:RDF a ioto:DataFormat ;
    rdfs:label "RDF" ;
    rdfs:comment "Resource Description Framework, a semantic web format for linked IoT data." .

# Define IoTStandard
ioto:IoTStandard a rdfs:Class ;
    rdfs:label "IoT Standard" ;
    rdfs:comment "Represents various IoT standards that can be dynamically introduced." .

# Define CommunicationProtocol
ioto:CommunicationProtocol a rdfs:Class ;
    rdfs:label "Communication Protocol" ;
    rdfs:comment "Represents communication protocols used by IoT devices." .

# Define hasStandard property
ioto:hasStandard a owl:ObjectProperty ;
    rdfs:domain ioto:IoTDevice ;
    rdfs:range ioto:IoTStandard ;
    rdfs:label "has Standard" ;
    rdfs:comment "Links an IoT device to a specific IoT standard it follows." .

# Define usesProtocol property
ioto:usesProtocol a owl:ObjectProperty ;
    rdfs:domain ioto:IoTDevice ;
    rdfs:range ioto:CommunicationProtocol ;
    rdfs:label "uses Protocol" ;
    rdfs:comment "Links an IoT device to a specific communication protocol it uses." .
    
